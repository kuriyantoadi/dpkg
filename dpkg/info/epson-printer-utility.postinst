#!/bin/bash
RPM_INSTALL_PREFIX=
export RPM_INSTALL_PREFIX
/sbin/ldconfig
# utility
ln -s /opt/epson-printer-utility/bin/epson-printer-utility /usr/bin/epson-printer-utility
install -m 0755 /opt/epson-printer-utility/rules/79-udev-epson.rules /etc/udev/rules.d
# backend
cd /usr/lib/epson-backend/scripts/
./inst-cups-post.sh install